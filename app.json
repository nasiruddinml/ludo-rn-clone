{
  "expo": {
    "name": "ludo",
    "slug": "ludo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mnuworld.ludo",
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1024.png",
        "backgroundColor": "#000000"
      },
      "package": "com.mnuworld.ludo",
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0fdccd01-8f02-4491-946b-dad4af0bb798"
      }
    },
    "owner": "mnuworld"
  }
}
